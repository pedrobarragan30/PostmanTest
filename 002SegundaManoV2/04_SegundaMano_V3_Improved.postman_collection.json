							"console.log(\"Pre-Encode: \" + preencode);",
							"console.log(\"Encoded: \" + encodedKeys);",
							"",
							"//Saving encodeKeys and mail to environment variables",
							"pm.environment.set(\"enconded\",encodedKeys);",
