							"let usuario = \"usuario\" + (Math.floor(Math.random() * 7685) + 3) + \"@viajes.com\";",
							"",
							"//Creating a random password of at least 5 characters",
							"let password = (Math.floor(Math.random() * 57684) + 10000);",
							"",
